HeaderFilterRegex: '.*'
Checks: '-*,
  clang-analyzer-*,
  cppcoreguidelines-*,
  bugprone-*,
  readability-*,
  portability-*,
  hicpp-*,
  concurrency-*,
  google-*,
  misc-*,
  modernize-*,
  -modernize-use-trailing-return-type,
  -misc-no-recursion,
  -readability-braces-around-statements,
  -google-readability-braces-around-statements,
  -hicpp-braces-around-statements'
FormatStyle: 'file'
